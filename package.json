{
  "name": "imoin",
  "version": "25.0.1",
  "description": "Icinga/Nagios status display",
  "main": "",
  "scripts": {
    "test": "npm run test:mocha && npm run test:karma",
    "test:mocha": "mocha -r ts-node/register test/test*.ts test/filters/*Tests.ts",
    "test:mocha:watch": "mocha -R min -w --watch-extensions ts -r ts-node/register test/test*.ts test/filters/*Tests.ts",
    "test:mocha:coverage": "nyc mocha -r ts-node/register -r source-map-support/register test/test*.ts test/filters/*Tests.ts",
    "test:karma": "karma start karma.conf.js",
    "test:karma:watch": "karma start karma.conf.js --no-single-run",
    "lint": "tslint -c tslint.json scripts/*.ts scripts/**/*.ts tests/*.ts tests/**/*.ts",
    "demoserver": "node test/data/server.js",
    "bump-version": "gulp bump",
    "bump-version-minor": "gulp bumpMinor",
    "build:firefox": "gulp firefox",
    "build:chrome": "gulp chrome",
    "build:edge": "gulp edge",
    "build:opera": "gulp opera",
    "watch:firefox": "npm run build:firefox && gulp firefoxWatch",
    "watch:chrome": "npm run build:chrome && gulp chromeWatch",
    "watch:edge": "npm run build:edge && gulp edgeWatch",
    "watch:opera": "npm run build:opera && gulp operaWatch",
    "package:firefox": "npm run build:firefox && cd release/firefox && web-ext build",
    "sign:firefox": "npm run package:firefox && cd release/firefox && web-ext sign --api-key=${AMO_JWT_ISSUER} --api-secret=${AMO_JWT_SECRET}",
    "run:firefox": "npm run build:firefox && cd release/firefox && web-ext run",
    "clean": "gulp clean"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "release",
      "html",
      "test",
      "coverage",
      "layouts",
      "gulpfile.js",
      "karma.conf.js",
      "webpack.config.js"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ErikWegner/imoin.git"
  },
  "keywords": [
    "Icinga",
    "Nagios"
  ],
  "author": "Erik Wegner",
  "license": "ISC",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "chai": "^4.3.10",
    "esbuild": "0.19.5",
    "eslint": "^8.53.0",
    "express": "^4.18.2",
    "gulp": "^4.0.2",
    "gulp-bump": "^3.2.0",
    "gulp-clean": "^0.4.0",
    "gulp-cli": "^2.3.0",
    "gulp-esbuild": "^0.11.2",
    "jasmine-core": "^5.1.1",
    "jasmine-sinon": "^0.4.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-sinon": "^1.0.4",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "semver": "^7.5.4",
    "sinon": "^17.0.1",
    "typescript": "^5.2.2",
    "web-ext": "^7.8.0"
  }
}
