{
  "name": "imoin",
  "version": "18.0.0",
  "description": "Icinga/Nagios status display",
  "main": "",
  "scripts": {
    "test": "mocha -r ts-node/register test/**/*.ts",
    "bump-version": "gulp bump",
    "bump-version-minor": "gulp bump-minor",
    "build:firefox": "gulp firefox",
    "build:chrome": "gulp chrome",
    "watch:firefox": "npm run build:firefox && gulp firefox-watch",
    "watch:chrome": "npm run build:chrome && gulp chrome-watch",
    "package:firefox": "npm run build:firefox && cd release/firefox && web-ext build",
    "package:chrome": "gulp clean-chrome && gulp chrome-package",
    "sign:firefox": "npm run package:firefox && web-ext sign --source-dir release/firefox --api-key=${npm_config_AMO_JWT_ISSUER} --api-secret=${npm_config_AMO_JWT_SECRET}",
    "run:firefox": "npm run build:firefox && cd release/firefox && web-ext run",
    "clean": "gulp clean"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ErikWegner/imoin.git"
  },
  "keywords": [
    "Icinga",
    "Nagios"
  ],
  "author": "Erik Wegner",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.43",
    "@types/sinon": "^2.3.6",
    "awesome-typescript-loader": "^3.3.0",
    "chai": "^4.1.2",
    "event-stream": "^3.3.4",
    "gulp": "^3.9.1",
    "gulp-bump": "^2.7.0",
    "gulp-clean": "^0.3.2",
    "gulp-typescript": "^3.1.4",
    "gulp-zip": "^4.0.0",
    "keytar": "^3.0.2",
    "mocha": "^4.0.1",
    "sinon": "^4.0.1",
    "ts-node": "^3.3.0",
    "typescript": "^2.1.5",
    "typings": "^2.1.1",
    "web-ext": "^1.8.1",
    "webpack": "^3.8.1",
    "webpack-stream": "^4.0.0"
  }
}
