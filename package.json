{
  "name": "imoin",
  "version": "24.1.0",
  "description": "Icinga/Nagios status display",
  "main": "",
  "scripts": {
    "test": "yarn run test:mocha && yarn run test:karma",
    "test:mocha": "mocha -r ts-node/register test/test*.ts test/filters/*Tests.ts",
    "test:mocha:watch": "mocha -R min -w --watch-extensions ts -r ts-node/register test/test*.ts test/filters/*Tests.ts",
    "test:mocha:coverage": "nyc mocha -r ts-node/register -r source-map-support/register test/test*.ts test/filters/*Tests.ts",
    "test:karma": "karma start karma.conf.js",
    "test:karma:watch": "karma start karma.conf.js --no-single-run",
    "lint": "tslint -c tslint.json scripts/*.ts scripts/**/*.ts tests/*.ts tests/**/*.ts",
    "demoserver": "node test/data/server.js",
    "panel-dev": "cross-env NODE_ENV=development webpack-dev-server --config layouts/panel/webpack.dev.js --content-base . --open-page layouts/panel/panel-dev.html --open --hot --watch",
    "panel-build": "webpack --config layouts/panel/webpack.prod.js",
    "bump-version": "gulp bump",
    "bump-version-minor": "gulp bump-minor",
    "build:firefox": "gulp firefox",
    "build:chrome": "gulp chrome",
    "build:edge": "gulp edge",
    "build:opera": "gulp opera",
    "watch:firefox": "yarn run build:firefox && gulp firefox-watch",
    "watch:chrome": "yarn run build:chrome && gulp chrome-watch",
    "watch:edge": "yarn run build:edge && gulp edge-watch",
    "watch:opera": "yarn run build:opera && gulp opera-watch",
    "package:firefox": "yarn run build:firefox && cd release/firefox && web-ext build",
    "sign:firefox": "yarn run package:firefox && cd release/firefox && web-ext sign --api-key=${AMO_JWT_ISSUER} --api-secret=${AMO_JWT_SECRET}",
    "run:firefox": "yarn run build:firefox && cd release/firefox && web-ext run",
    "clean": "gulp clean"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "release",
      "html",
      "test",
      "coverage",
      "layouts",
      "gulpfile.js",
      "karma.conf.js",
      "webpack.config.js"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ErikWegner/imoin.git"
  },
  "keywords": [
    "Icinga",
    "Nagios"
  ],
  "author": "Erik Wegner",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.53",
    "@types/sinon": "^5.0.1",
    "awesome-typescript-loader": "^3.4.0",
    "babel-loader": "^8.0.4",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "event-stream": "^3.3.4",
    "express": "^4.16.2",
    "gulp": "^3.9.1",
    "gulp-bump": "^2.7.0",
    "gulp-clean": "^0.3.2",
    "gulp-typescript": "^3.1.4",
    "gulp-zip": "^4.0.0",
    "jasmine-core": "^2.8.0",
    "jasmine-sinon": "^0.4.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-sinon": "^1.0.4",
    "mocha": "^4.0.1",
    "nyc": "^12.0.2",
    "sinon": "^4.0.1",
    "source-map-support": "^0.5.6",
    "ts-node": "^6.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.6.1",
    "web-ext": "^1.8.1",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4",
    "webpack-stream": "^4.0.0"
  }
}
