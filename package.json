{
  "name": "imoin",
  "version": "21.1.1",
  "description": "Icinga/Nagios status display",
  "main": "",
  "scripts": {
    "test": "npm run test:mocha && npm run test:karma",
    "test:mocha": "mocha -r ts-node/register test/test*.ts test/filters/*Tests.ts",
    "test:mocha:watch": "mocha -w --watch-extensions ts -r ts-node/register test/test*.ts test/filters/*Tests.ts",
    "test:karma": "karma start karma.conf.js",
    "test:karma:watch": "karma start karma.conf.js --no-single-run",
    "lint": "tslint -c tslint.json scripts/*.ts scripts/**/*.ts tests/*.ts tests/**/*.ts",
    "demoserver": "node test/data/server.js",
    "panel-dev": "cross-env NODE_ENV=development webpack-dev-server --config html/webpack.config.js --content-base . --open-page html/panel-dev.html --open --hot",
    "bump-version": "gulp bump",
    "bump-version-minor": "gulp bump-minor",
    "build:firefox": "gulp firefox",
    "build:chrome": "gulp chrome",
    "build:edge": "gulp edge",
    "build:opera": "gulp opera",
    "watch:firefox": "npm run build:firefox && gulp firefox-watch",
    "watch:chrome": "npm run build:chrome && gulp chrome-watch",
    "watch:edge": "npm run build:edge && gulp edge-watch",
    "watch:opera": "npm run build:opera && gulp opera-watch",
    "package:firefox": "npm run build:firefox && cd release/firefox && web-ext build",
    "sign:firefox": "npm run package:firefox && cd release/firefox && web-ext sign --api-key=${AMO_JWT_ISSUER} --api-secret=${AMO_JWT_SECRET}",
    "run:firefox": "npm run build:firefox && cd release/firefox && web-ext run",
    "clean": "gulp clean"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ErikWegner/imoin.git"
  },
  "keywords": [
    "Icinga",
    "Nagios"
  ],
  "author": "Erik Wegner",
  "license": "ISC",
  "dependencies": {
    "vue": "^2.5.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.53",
    "@types/sinon": "^2.3.6",
    "awesome-typescript-loader": "^3.4.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.1.2",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "event-stream": "^3.3.4",
    "express": "^4.16.2",
    "file-loader": "^1.1.4",
    "gulp": "^3.9.1",
    "gulp-bump": "^2.7.0",
    "gulp-clean": "^0.3.2",
    "gulp-typescript": "^3.1.4",
    "gulp-zip": "^4.0.0",
    "jasmine-core": "^2.8.0",
    "jasmine-sinon": "^0.4.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-sinon": "^1.0.4",
    "mocha": "^4.0.1",
    "sinon": "^4.0.1",
    "ts-node": "^6.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.6.1",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "web-ext": "^1.8.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.1",
    "webpack-stream": "^4.0.0"
  }
}
